<?xml version="1.0" encoding="UTF-8"?>
<ISS>
    <Calibration>
        <Sensor SensorTypeId="65" SubTypeId="32183" VendorId="2" InstanceId="0" Flags="2">
            <!--Below are the calibration parameters to be applied to raw readings. Note that it is not an issue that after calibration, sensor readings might exceed original range.-->
            <CalibrationData Version="0" FormatId="1">
                <!--The following represents the scale the sensor data will be multiplied by. The scale is a floating number is represented by multiplier/divider. The final result is Lux value read from sensor * (multiplier/divider)-->
                <Multiplier Divider="100">934</Multiplier>
                <Steps NumOfSteps="4">
                    <Step>
                        <Scale value="60"/>
                        <Lux value="10"/>
                    </Step>
                    <Step>
                        <Scale value="100"/>
                        <Lux value="11"/>
                    </Step>
                    <Step>
                        <Scale value="100"/>
                        <Lux value="8000"/>
                    </Step>
                    <Step>
                        <Scale value="500"/>
                        <Lux value="8001"/>
                    </Step>
                </Steps>
            </CalibrationData>
        </Sensor>
    </Calibration>
</ISS>
